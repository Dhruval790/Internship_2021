{
    "source": {
        "include": ["./src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)",
        "exclude":["./src/setupTests.js"]
    },
    "plugins": ["plugins/markdown", "better-docs/component"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
      },
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    }
}